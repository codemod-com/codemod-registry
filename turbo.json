{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build:cjs": {
			"dependsOn": ["^build:cjs"],
			"outputMode": "full",
			"inputs": ["./src/**/*.ts", "./src/**/*.js"],
			"outputs": ["./dist/index.cjs"]
		},
		"create": {
			"dependsOn": ["^create"],
			"cache": false
		},
		"test": {
			"dependsOn": ["^test"],
			"cache": false
		}
	}
}
